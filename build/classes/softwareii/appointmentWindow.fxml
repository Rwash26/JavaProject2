<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="509.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="softwareii.AppointmentController">
   <children>
      <TableView fx:id="appointmentTable" prefHeight="410.0" prefWidth="455.0">
        <columns>
          <TableColumn fx:id="appointmentIdCol" prefWidth="37.0" text="ID" />
          <TableColumn fx:id="titleCol" prefWidth="109.0" text="Title" />
            <TableColumn fx:id="startCol" prefWidth="152.0" text="Start" />
            <TableColumn fx:id="endCol" prefWidth="156.0" text="End" />
        </columns>
      </TableView>
      <Button fx:id="returnButton" layoutX="340.0" layoutY="434.0" mnemonicParsing="false" onAction="#returnButtonAction" text="Return" />
      <Button fx:id="addButton" layoutX="55.0" layoutY="434.0" mnemonicParsing="false" onAction="#addAppointmentButtonAction" text="Add" />
      <Button fx:id="modifyButton" layoutX="115.0" layoutY="434.0" mnemonicParsing="false" onAction="#modifyAppointmentButtonAction" text="Modify" />
      <Button fx:id="filterButton" layoutX="184.0" layoutY="434.0" mnemonicParsing="false" onAction="#filterAppointmentButtonAction" text="By Week" />
      <Button fx:id="filterButton2" layoutX="259.0" layoutY="434.0" mnemonicParsing="false" onAction="#filterAppointmentButtonAction2" text="By Month" />
   </children>
</AnchorPane>
